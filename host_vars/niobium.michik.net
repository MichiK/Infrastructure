fqdn: niobium.michik.net
use_ntp: false
document_root: "/srv/http/michik.net/web"
dh_param_length: 2048

acmetool_cert_domains: "niobium.michik.net michik.net c3bottl.es"
apache_force_https: false
apache_additional_default_vhost_settings: |
  <Directory /srv/http/michik.net/web/>
    Options Indexes FollowSymLinks
    AllowOverride None
    Require all granted
  </Directory>
  WSGIScriptAlias /cards /srv/http/michik.net/web/cards/cards.wsgi

prosody_ssl_ciphers: "ECDHE-RSA-AES256-GCM-SHA384:DHE-RSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-SHA384:DHE-RSA-AES256-SHA256:ECDHE-RSA-AES256-SHA:DHE-RSA-AES256-SHA:ECDHE-RSA-CAMELLIA256-SHA384:DHE-RSA-CAMELLIA256-SHA:AES256-GCM-SHA384:AES256-SHA"
prosody_hosts:
 - michik.net
